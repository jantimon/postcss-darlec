{
  "name": "postcss-darlec",
  "version": "0.0.5",
  "description": "allow to lint preprocessed css",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jantimon/postcss-darlec"
  },
  "scripts": {
    "test": "istanbul cover _mocha --report html -- test/*.js --reporter spec",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "keywords": [
    "css",
    "node",
    "postcss"
  ],
  "author": "Jan Nicklas <j.nicklas@me.com> (https://github.com/jantimon)",
  "license": "MIT",
  "engines": {
    "node": ">=0.12"
  },
  "dependencies": {
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "minimatch": "^3.0.0",
    "path-sort": "^0.1.0",
    "postcss": "^5.0.10",
    "urldata": "0.0.1"
  },
  "bin": {
    "postcss-darlec": "bin/postcss-darlec"
  },
  "devDependencies": {
    "coveralls": "^2.11.4",
    "istanbul": "^0.4.0",
    "mocha": "^2.3.3",
    "node-sass": "^3.4.1"
  },
  "bugs": {
    "url": "https://github.com/jantimon/postcss-darlec/issues"
  },
  "homepage": "https://github.com/jantimon/postcss-darlec"
}
